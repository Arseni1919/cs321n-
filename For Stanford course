# !! Contents within this block are managed by 'conda init' !!
#-------------------------
#uncomment this:
#-------------------------
__conda_setup="$('/home/hamster/anaconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/hamster/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/home/hamster/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/home/hamster/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
